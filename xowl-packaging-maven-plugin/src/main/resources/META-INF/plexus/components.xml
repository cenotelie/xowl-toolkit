            <configuration>
                <type>xowl-platform</type>
                <extension>tar.gz</extension>
                <packaging>xowl-platform</packaging>
                <classifier>xowl-platform</classifier>
                <language>java</language>
                <addedToClasspath>false</addedToClasspath>
            </configuration>
